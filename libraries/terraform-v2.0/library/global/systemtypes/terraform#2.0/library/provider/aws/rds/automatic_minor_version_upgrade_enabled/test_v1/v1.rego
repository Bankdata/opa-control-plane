test_auto_minor_version_upgrade_rds_good {
	auto_minor_version_upgrade := true
	in := input_rds_instance(auto_minor_version_upgrade)
	actual := v1.enable_auto_minor_version_upgrade_rds_instance with input as in
	count(actual) == 0
}

test_auto_minor_version_upgrade_rds_bad {
	auto_minor_version_upgrade := false
	in := input_rds_instance(auto_minor_version_upgrade)
	actual := v1.enable_auto_minor_version_upgrade_rds_instance with input as in
	count(actual) == 1
}

