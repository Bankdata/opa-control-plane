# Ensuring the output which is list of error message contains only one error message which
# is for resource `bad_example` while the input contains good resource = example.
test_codebuild_project_privileged_check_bad {
	privilege_mode := true
	in := input_for_privilege_mode_check(privilege_mode)
	actual := v1.codebuild_project_privileged_check with input as in
	count(actual) == 1
}

test_codebuild_project_privileged_check_good {
	privilege_mode := false
	in := input_for_privilege_mode_check(privilege_mode)
	actual := v1.codebuild_project_privileged_check with input as in
	count(actual) == 0
}

